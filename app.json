{
  "expo": {
    "name": "VaultedSlabs",
    "slug": "vaultedslabs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vaultedslabs",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vaultedslabs.app"
    },
    "android": {
      "package": "com.vaultedslabs.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow VaultedSlabs to access your camera to scan PSA labels"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
